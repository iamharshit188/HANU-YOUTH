  "scripts": {
    "server": "cross-env HOST=localhost SOCKET_PORT=3001 NODE_OPTIONS=--no-warnings=ExperimentalWarning npx tsx server.ts",
    "server:watch": "cross-env HOST=localhost SOCKET_PORT=3001 NODE_OPTIONS=--no-warnings=ExperimentalWarning nodemon --config nodemon.json --exec \"npx tsx server.ts\"",
    "dev": "cross-env NODE_OPTIONS=--no-warnings=ExperimentalWarning concurrently \"npx next dev\" \"npm:server:watch\"",
    "dev:concurrent": "cross-env NODE_OPTIONS=--no-warnings=ExperimentalWarning concurrently \"npx next dev\" \"cross-env HOST=localhost nodemon --watch server.ts --watch src/lib --watch lib --ext ts,tsx,js,jsx,json --delay 0 --exec 'npx tsx server.ts'\"",
    "build": "next build",
    "export": "next export",
    "postbuild": "next-sitemap",
    "build:netlify": "prisma generate && SKIP_ENV_VALIDATION=true npm run build && npm run export && npx next-sitemap",
    "start": "NODE_ENV=production next start -p $PORT",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "deploy:netlify": "./deploy-netlify.sh",
    "deploy:netlify:win": "deploy-netlify.bat"
  },
